{
  "name": "@lit/vocab-term",
  "version": "0.1.0",
  "description": "Utility library for working with RDF vocabulary terms (e.g. IRI's or literals (including multi-lingual literals)). Wraps rdf-ext.",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/inrupt/lit-vocab-term-js"
  },
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "mkdir -p test/resources/output && nyc mocha \"./test/**/*.test.js\"",
    "testX": "mkdir -p test/resources/output && nyc mocha \"../../test/**/*.test.js\" \"./test/**/*.test.js\"",
    "test-explicitly-exclude-node-modules": "mkdir -p test/resources/output && nyc mocha \"../../{,!(node_modules)/**/}*.test.js\" \"./{,!(node_modules)/**/}*.test.js\""
  },
  "dependencies": {
    "@rdfjs/formats-common": "^2.0.1",
    "aggregation": "^1.2.5",
    "moment": "^2.24.0",
    "rdf-ext": "^1.3.0",
    "stream-to-string": "^1.2.0",
    "string-to-stream": "^3.0.1"
  },
  "author": "Pat McBennett",
  "license": "MIT",
  "nyc": {
    "check-coverage": true,
    "per-file": true,
    "lines": 98,
    "statements": 98,
    "functions": 98,
    "branches": 98,
    "include": [
      "src/**/*.js"
    ],
    "exclude": [],
    "reporter": [
      "lcov",
      "text"
    ],
    "require": [],
    "extension": [],
    "cache": true,
    "all": true,
    "report-dir": "./.nyc_output/alternative"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-string": "^1.5.0",
    "mocha": "^6.1.4",
    "mock-local-storage": "^1.1.8",
    "nyc": "^14.1.1",
    "standard": "^12.0.1"
  }
}
