{
  "name": "@pmcb55/lit-vocab-term-rdf-ext",
  "version": "0.2.4",
  "description": "Extends the base LIT Vocab Term library to wrap rdf-ext.",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/pmcb55/lit-js.git",
    "directory": "lit-vocab-term/lit-vocab-term-rdf-ext"
  },
  "directories": {
    "test": "test"
  },
  "scripts": {
    "format": "prettier --write \"index.js\" \"src/**/*.js\"",
    "test": "eslint index.js \"src/**\" && mkdir -p test/resources/output && nyc mocha \"./test/**/*.test.js\"",
    "preversion": "npm test"
  },
  "dependencies": {
    "@pmcb55/lit-vocab-term-base": "0.2.4",
    "@rdfjs/formats-common": "^2.0.1",
    "aggregation": "^1.2.5",
    "moment": "^2.24.0",
    "debug": "^4.1.1",
    "rdf-ext": "^1.3.0",
    "stream-to-string": "^1.2.0",
    "string-to-stream": "^3.0.1"
  },
  "author": "Pat McBennett",
  "license": "MIT",
  "nyc": {
    "check-coverage": true,
    "per-file": true,
    "lines": 98,
    "statements": 98,
    "functions": 98,
    "branches": 98,
    "include": [
      "src/**/*.js"
    ],
    "exclude": [],
    "reporter": [
      "lcov",
      "text"
    ],
    "require": [],
    "extension": [],
    "cache": true,
    "all": true,
    "report-dir": "./.nyc_output/alternative"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-string": "^1.5.0",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.8",
    "mocha": "^6.1.4",
    "mock-local-storage": "^1.1.8",
    "nyc": "^14.1.1",
    "prettier": "2.0.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --cache --fix",
      "prettier --write"
    ]
  }
}
