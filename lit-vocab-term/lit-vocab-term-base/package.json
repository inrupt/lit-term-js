{
  "name": "@pmcb55/lit-vocab-term-base",
  "version": "0.2.4",
  "description": "Very basic implementation of LIT Vocab Term with no RDF library dependencies at all.",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/pmcb55/lit-js.git",
    "directory": "lit-vocab-term/lit-vocab-term-base"
  },
  "directories": {
    "test": "test"
  },
  "scripts": {
    "format": "prettier --write \"index.js\" \"src/**/*.js\"",
    "test": "nyc mocha \\\"./test/**/*.test.js\\\"",
    "preversion": "npm test"
  },
  "dependencies": {
    "debug": "^4.1.1",
    "moment": "^2.24.0"
  },
  "author": "Pat McBennett",
  "license": "MIT",
  "nyc": {
    "check-coverage": true,
    "per-file": true,
    "lines": 98,
    "statements": 98,
    "functions": 98,
    "branches": 98,
    "include": [
      "src/**/*.js"
    ],
    "exclude": [],
    "reporter": [
      "lcov",
      "text"
    ],
    "require": [],
    "extension": [],
    "cache": true,
    "all": true,
    "report-dir": "./.nyc_output/alternative"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-string": "^1.5.0",
    "mocha": "^6.1.4",
    "mock-local-storage": "^1.1.8",
    "nyc": "^14.1.1",
    "rdf-ext": "^1.3.0",
    "standard": "^12.0.1"
  }
}
