{
  "name": "@inrupt/lit-vocab-term",
  "version": "0.2.5",
  "description": "Very basic implementation of LIT Vocab Term, with minimal RDF library dependencies.",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/inrupt/lit-vocab-term-js.git",
    "directory": "lit-vocab-term/lit-vocab-term-base"
  },
  "directories": {
    "test": "test"
  },
  "scripts": {
    "format": "prettier --write \"src/**/*.ts\"",
    "test": "eslint --config .eslintrc.js \"src/**\" && jest --no-cache ./src",
    "preversion": "npm test",
    "build": "rollup --config rollup.config.js",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@rdfjs/data-model": "1.1.2",
    "@types/debug": "^4.1.5",
    "@types/rdf-js": "2.0.12",
    "debug": "^4.1.1"
  },
  "author": {
    "name": "Pat McBennett"
  },
  "contributors": [{
    "name": "Nicolas Seydoux"
  }],
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^11.0.2",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "@types/chai": "^4.2.11",
    "@types/jest": "^25.1.4",
    "@typescript-eslint/eslint-plugin": "^2.25.0",
    "@typescript-eslint/parser": "^2.25.0",
    "chai": "^4.2.0",
    "chai-string": "^1.5.0",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^4.2.3",
    "jest": "^25.2.1",
    "lint-staged": "^10.1.1",
    "prettier": "2.0.2",
    "rollup": "^2.2.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-typescript2": "^0.26.0",
    "ts-jest": "^25.2.1",
    "typescript": "^3.8.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --cache --fix",
      "prettier --write"
    ]
  }
}
