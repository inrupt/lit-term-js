{
  "name": "@lit/vocab-term-base",
  "version": "0.2.0",
  "description": "Base utility library for working with RDF vocabulary terms (e.g. IRI's or literals (including multi-lingual literals)). Wraps rdf-ext.",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/inrupt/lit-vocab-term-js"
  },
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "mkdir -p test/resources/output && nyc mocha \"./test/**/*.test.js\""
  },
  "dependencies": {
    "debug": "^4.1.1",
    "moment": "^2.24.0"
  },
  "author": "Pat McBennett",
  "license": "MIT",
  "nyc": {
    "check-coverage": true,
    "per-file": true,
    "lines": 98,
    "statements": 98,
    "functions": 98,
    "branches": 98,
    "include": [
      "src/**/*.js"
    ],
    "exclude": [],
    "reporter": [
      "lcov",
      "text"
    ],
    "require": [],
    "extension": [],
    "cache": true,
    "all": true,
    "report-dir": "./.nyc_output/alternative"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-string": "^1.5.0",
    "mocha": "^6.1.4",
    "mock-local-storage": "^1.1.8",
    "nyc": "^14.1.1",
    "rdf-ext": "^1.3.0",
    "standard": "^12.0.1"
  }
}
